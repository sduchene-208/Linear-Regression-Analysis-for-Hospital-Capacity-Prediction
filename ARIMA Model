ods noproctitle;
ods graphics / imagemap=on;

proc sort data=WORK.IMPORT out=Work.preProcessedData; *idaho
	by reporting_date;
run;

proc arima data=Work.preProcessedData plots(only)=(series(corr) residual(corr 
		normal) forecast(forecast));
	identify var=inpatient_beds_utilization(1);
	estimate method=ML;
	forecast lead=12 back=0 alpha=0.05 id=reporting_date interval=day;
	outlier;
	run;
quit;

proc delete data=Work.preProcessedData;
run;

proc sort data=WORK.IMPORT1 out=Work.preProcessedData; *oregon
	by reporting_date;
run;

proc arima data=Work.preProcessedData plots(only)=(series(corr) residual(corr 
		normal) forecast(forecast));
	identify var=inpatient_beds_utilization(1);
	estimate method=ML;
	forecast lead=12 back=0 alpha=0.05 id=reporting_date interval=day;
	outlier;
	run;
quit;

proc delete data=Work.preProcessedData;
run;

proc sort data=WORK.IMPORT1 out=Work.preProcessedData; *oregon
	by reporting_date;
run;

proc arima data=Work.preProcessedData plots(only)=(series(corr) residual(corr 
		normal) forecast(forecast));
	identify var=percent_of_inpatients_with_covid(1);
	estimate method=ML;
	forecast lead=12 back=0 alpha=0.05 id=reporting_date interval=day;
	outlier;
	run;
quit;

proc delete data=Work.preProcessedData;
run;

proc sort data=WORK.IMPORT1 out=Work.preProcessedData; *oregon
	by reporting_date;
run;

proc arima data=Work.preProcessedData plots(only)=(series(corr) residual(corr 
		normal) forecast(forecast));
	identify var=inpatient_bed_covid_utilization(1);
	estimate method=ML;
	forecast lead=12 back=0 alpha=0.05 id=reporting_date interval=day;
	outlier;
	run;
quit;

proc delete data=Work.preProcessedData;
run;

proc sort data=WORK.IMPORT1 out=Work.preProcessedData; *oregon
	by reporting_date;
run;

proc arima data=Work.preProcessedData plots(only)=(series(corr) residual(corr 
		normal) forecast(forecast));
	identify var=adult_icu_bed_utilization(1);
	estimate method=ML;
	forecast lead=12 back=0 alpha=0.05 id=reporting_date interval=day;
	outlier;
	run;
quit;

proc delete data=Work.preProcessedData;
run;
