ods noproctitle;
ods graphics / imagemap=on;

proc means data=WORK.IMPORT1 chartype mean std min max n nmiss vardef=df cv 
		skewness kurtosis;
		title height=14pt "Summary Statistics for Idaho";
	var inpatient_beds_utilization percent_of_inpatients_with_covid 
		inpatient_bed_covid_utilization adult_icu_bed_covid_utilization 
		adult_icu_bed_utilization;
run;

proc means data=WORK.IMPORT chartype mean std min max n nmiss vardef=df cv 
		skewness kurtosis;
		title height=14pt "Summary Statistics for Oregon";
	var inpatient_beds_utilization percent_of_inpatients_with_covid 
		inpatient_bed_covid_utilization adult_icu_bed_covid_utilization 
		adult_icu_bed_utilization;
run;
